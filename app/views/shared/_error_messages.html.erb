<% if target.errors.any? %>
<div class="error_explanation">
  <div class="alert alert-danger">
    <h2><%= pluralize(target.errors.count, "error") %> prevented this record from being saved:</h2>
  </div>
  <ul>
  <% target.errors.full_messages.each do |msg| %>
    <li><%= msg %></li>
  <% end %>
  </ul>
</div>
<% end %>
